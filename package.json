{
  "name": "skitkit",
  "version": "999.999.999",
  "engines": {
    "node": ">=12.13.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/skitkit"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "3.6.3",
    "@types/mini-css-extract-plugin": "0.9.1",
    "@types/node": "13.13.14",
    "@types/rewire": "2.5.28",
    "@types/superfine": "7.0.1",
    "@types/uuid": "^8.0.0",
    "@types/workbox-webpack-plugin": "5.1.1",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "css-loader": "3.6.0",
    "del-cli": "3.0.1",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "favicons-webpack-plugin": "3.0.1",
    "html-webpack-plugin": "4.3.0",
    "jasmine": "3.5.0",
    "mini-css-extract-plugin": "0.9.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "pug": "3.0.0",
    "pug-loader": "2.4.0",
    "rewire": "5.0.0",
    "sass": "1.26.10",
    "sass-loader": "9.0.2",
    "superfine": "7.0.0",
    "typescript": "3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "workbox-webpack-plugin": "5.1.3"
  },
  "scripts": {
    "buildClean": "del-cli \"**/*.js\" \"**/*.d.ts\" \"!**/node_modules/**/*\"",
    "buildTypeScript": "tsc",
    "buildJasmineUnit": "jasmine --config=unit.jasmine.json",
    "buildNyc": "nyc --check-coverage --all --exclude \"**/*.ts\" --exclude \"**/*.d.ts\" --exclude \"**/unit.*\" --exclude \"editor/index.js\" --exclude \"editor/webpack.config.js\" --lines 100 --functions 100 --branches 100 --statements 100 npm run-script buildJasmineUnit",
    "test": "npm-run-all buildClean buildTypeScript buildNyc",
    "lint": "eslint \"**/*.ts\"",
    "webpack": "webpack --mode=production --config ./editor/webpack.config.js"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.js",
    "!**/unit.d.ts",
    "!editor"
  ],
  "dependencies": {
    "jsonschema": "1.2.6",
    "uuid": "8.2.0"
  }
}
